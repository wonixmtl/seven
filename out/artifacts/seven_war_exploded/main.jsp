<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<html>
    <head>
        <title>七班大乱斗</title>
        <link rel="stylesheet" type="text/css" href="css/bootstrap.css">
        <link rel="stylesheet" type="text/css" href="css/normalize.css">
        <script type="text/javascript" src="js/jquery-3.4.1.js"></script>
        <script type="text/javascript" src="js/bootstrap.js"></script>
    </head>
    <body>
        <div class="container" style="height: 600px;width: 600px;margin-top: 50px;">
            <%-- 第一行 --%>
            <div class="row">
                <div class="col-md-3" style="height: 300px;width: 150px;background-color: yellow">凉州</div>
                <div class="col-md-5">
                    <div class="row">
                        <div class="col-md-7" style="height: 200px;width: 250px;background-color: blue">并州</div>
                    </div>
                    <div class="row" style="width: 250px;">
                        <div class="col-md-6" style="height: 100px;width: 125px;background-color: #fdffee">雍州</div>
                        <div class="col-md-6" style="height: 100px;width: 125px;background-color: gray">司隶</div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="row">
                        <div class="col-md-4" style="height: 150px;width: 200px;background-color: plum">幽州</div>
                    </div>
                    <div class="row">
                        <div class="col-md-4" style="height: 150px;width: 200px;background-color: orange">青州</div>
                    </div>
                </div>
            </div>

            <%-- 第二行 --%>
            <div class="row">
                <div class="col-md-3" style="height: 300px;width: 150px;background-color: red">益州</div>
                <div class="col-md-5">
                    <div class="row">
                        <div class="col-md-6" style="height: 100px;width: 125px;background-color: green">豫州</div>
                        <div class="col-md-6" style="height: 100px;width: 125px;background-color: oldlace">兖州</div>
                    </div>
                    <div class="row">
                        <div class="col-md-12" style="height: 200px;width: 250px;background-color: deepskyblue">荆州</div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="row">
                        <div class="col-md-4" style="height: 150px;width: 200px;background-color: #78ddbf">徐州</div>
                    </div>
                    <div class="row">
                        <div class="col-md-4" style="height: 150px;width: 200px;background-color: #ffe0c6">扬州</div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>
